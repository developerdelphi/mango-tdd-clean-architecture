{
  "name": "mango-clean-tdd",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --watch --silent --colors --noStackTrace",
    "test:staged": "jest --passWithNoTests --findRelatedTests ./src/* --silent --noStackTrace",
