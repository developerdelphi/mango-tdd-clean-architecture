  "standard": {
    "env": [
      "jest"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "standard": "^16.0.3"
  }
}
